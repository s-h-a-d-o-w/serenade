{
  "name": "Serenade",
  "productName": "Serenade",
  "version": "2.0.4",
  "type": "module",
  "author": "s-h-a-d-o-w",
  "description": "Code with voice. Learn more at https://serenade.ai",
  "license": "MIT",
  "private": true,
  "main": "./out/main.js",
  "scripts": {
    "build:esbuild": "rimraf out && ts esbuild.ts && npm run build:renderer:vite",
    "build": "rimraf out && npm-run-all build:main:vite build:renderer:vite",
    "build:webpack": "rimraf out && npm-run-all build:main build:renderer",
    "build:main": "webpack --config main.webpack.ts --mode=production",
    "build:main:vite": "vite build",
    "build:renderer": "webpack --config renderer.webpack.ts --mode=production",
    "build:renderer:vite": "vite build --config src/renderer/vite.config.ts",
    "clean": "rm -rf src/gen dist out static/custom-commands-server/node_modules",
    "dev:esbuild": "npm-run-all -p dev:renderer:vite wait-on:renderer:esbuild",
    "dev": "npm-run-all -p dev:renderer:vite wait-on:renderer:vite",
    "dev:webpack": "npm-run-all -p dev:renderer wait-on:renderer",
    "wait-on:renderer": "wait-on http-get://localhost:4000/ && npm run dev:main",
    "wait-on:renderer:esbuild": "wait-on http-get://localhost:4000/ && npm run dev:main:esbuild",
    "wait-on:renderer:vite": "wait-on http-get://localhost:4000/ && npm run dev:main:vite",
    "dev:main": "cross-env NODE_ENV=development webpack --config main.webpack.ts --mode=development && npm run start:electron",
    "dev:main:esbuild": "ts esbuild.ts  && npm run start:electron",
    "dev:main:vite": "vite build && npm run start:electron",
    "dev:renderer": "cross-env NODE_ENV=development webpack-dev-server --config renderer.webpack.ts --mode=development",
    "dev:renderer:vite": "vite --config src/renderer/vite.config.ts",
    "bundle": "rimraf dist && rimraf out && npm-run-all build bundle:dist",
    "bundle:unsigned": "cross-env SKIP_SIGN=1 npm-run-all build bundle:distunsigned",
    "bundle:dist": "electron-builder",
    "bundle:distunsigned": "electron-builder -c.mac.identity=null",
    "release": "npm-run-all build release:publish",
    "release:publish": "electron-builder --publish always",
    "start:electron": "electron .",
    "typecheck": "tsc --noEmit"
  },
  "build": {
    "afterSign": "bin/notarize.js",
    "asar": false,
    "npmRebuild": false,
    "mac": {
      "category": "public.app-category.developer-tools",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./build/entitlements.mac.plist",
      "entitlementsInherit": "./build/entitlements.mac.plist",
      "signIgnore": "(static/local/jdk/lib/modules)|(static/local/jdk/lib/security/cacerts)|(static/local/code-engine-models)|(static/local/speech-engine-models)",
      "extendInfo": {
        "NSAppleEventsUsageDescription": "Serenade uses Apple Events to automate your workflow.",
        "NSMicrophoneUsageDescription": "Serenade uses your microphone to enable you to code with voice."
      },
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "dmg": {
      "title": "Serenade"
    },
    "protocols": {
      "name": "serenade-protocol",
      "schemes": [
        "serenade"
      ]
    },
    "publish": {
      "provider": "generic",
      "url": "https://serenadecdn.com/app",
      "useMultipleRangeRequest": false
    }
  },
  "electronWebpack": {
    "whiteListedModules": [
      "react-redux"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.17",
    "builder-util-runtime": "^9.3.1",
    "chokidar": "^4.0.3",
    "classnames": "^2.5.1",
    "codemirror-minified": "^5.65.9",
    "command-exists": "^1.2.9",
    "electron-fetch": "^1.9.1",
    "electron-updater": "^6.6.2",
    "fs-extra": "^11.3.2",
    "glob": "^12.0.0",
    "jsonc": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "protobufjs": "^7.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.6",
    "react-string-replace": "^1.1.1",
    "redux": "^5.0.1",
    "semver": "^7.7.3",
    "serenade-driver": "1.1.13",
    "speech-recorder": "2.1.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@types/command-exists": "^1.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@types/semver": "^7.7.1",
    "@types/webpack-node-externals": "^3.0.4",
    "@types/ws": "^8.18.1",
    "@typescript/native-preview": "7.0.0-dev.20251118.1",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "copy-webpack-plugin": "^13.0.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.3",
    "electron": "39.2.2",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.27.0",
    "esbuild-plugin-copy": "^2.1.1",
    "globals": "^16.5.0",
    "html-webpack-plugin": "^5.6.5",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.6.2",
    "protobufjs-cli": "^1.1.3",
    "rimraf": "^6.1.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.17",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^5.9.3",
    "uglify-js": "^3.19.3",
    "vite": "^7.2.2",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-static-copy": "^3.1.4",
    "wait-on": "^9.0.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.3.3"
  }
}
