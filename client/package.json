{
  "name": "Serenade",
  "productName": "Serenade",
  "version": "2.0.4",
  "type": "module",
  "author": "s-h-a-d-o-w",
  "description": "Code with voice. Learn more at https://serenade.ai",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "./out/main.js",
  "scripts": {
    "build": "pnpm run setup && run-p build:main build:renderer",
    "build:core": "pbjs -t static-module -o src/gen/core.js -w es6 ../toolbelt/src/main/proto/core.proto && pbts -o src/gen/core.d.ts src/gen/core.js",
    "build:main": "vite build",
    "build:renderer": "vite build --config vite.config.renderer.ts",
    "build:customcommands": "vite build --config vite.config.customcommands.ts",
    "bundle": "rimraf dist && pnpm build && electron-builder",
    "bundle:unsigned": "pnpm build && cross-env SKIP_SIGN=1 electron-builder -c.mac.identity=null",
    "check:core": "node --experimental-transform-types scripts/checkCore.ts",
    "check:customcommands": "node --experimental-transform-types scripts/checkCustomCommands.ts",
    "dev": "pnpm run setup && run-p dev:renderer wait-on:renderer",
    "dev:renderer": "vite --config vite.config.renderer.ts",
    "release": "npm-run-all build release:publish",
    "release:publish": "electron-builder --publish always",
    "setup": "rimraf out && run-p check:core check:customcommands",
    "start:electron": "cross-env ELECTRON_ENABLE_LOGGING=1 electron .",
    "typecheck": "tsc --noEmit",
    "wait-on:renderer": "wait-on http-get://localhost:4000/ && cross-env NODE_ENV=development vite build && pnpm start:electron"
  },
  "build": {
    "afterSign": "scripts/notarize.js",
    "asar": false,
    "npmRebuild": false,
    "mac": {
      "category": "public.app-category.developer-tools",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./build/entitlements.mac.plist",
      "entitlementsInherit": "./build/entitlements.mac.plist",
      "signIgnore": "(static/local/jdk/lib/modules)|(static/local/jdk/lib/security/cacerts)|(static/local/code-engine-models)|(static/local/speech-engine-models)",
      "extendInfo": {
        "NSAppleEventsUsageDescription": "Serenade uses Apple Events to automate your workflow.",
        "NSMicrophoneUsageDescription": "Serenade uses your microphone to enable you to code with voice."
      },
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "dmg": {
      "title": "Serenade"
    },
    "protocols": {
      "name": "serenade-protocol",
      "schemes": [
        "serenade"
      ]
    },
    "publish": {
      "provider": "generic",
      "url": "https://serenadecdn.com/app",
      "useMultipleRangeRequest": false
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "chokidar": "^4.0.3",
    "classnames": "^2.5.1",
    "codemirror-minified": "^5.65.9",
    "command-exists": "^1.2.9",
    "electron-updater": "^6.6.2",
    "fs-extra": "^11.3.2",
    "glob": "^12.0.0",
    "jsonc": "^2.0.0",
    "protobufjs": "^7.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.6",
    "react-string-replace": "^1.1.1",
    "redux": "^5.0.1",
    "semver": "^7.7.3",
    "serenade-driver": "1.1.13",
    "speech-recorder": "2.1.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/command-exists": "^1.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@types/semver": "^7.7.1",
    "@types/ws": "^8.18.1",
    "@typescript/native-preview": "7.0.0-dev.20251118.1",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.3",
    "electron": "39.2.2",
    "electron-builder": "^26.0.12",
    "globals": "^16.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.6.2",
    "protobufjs-cli": "^1.1.3",
    "rimraf": "^6.1.2",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.17",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^5.9.3",
    "uglify-js": "^3.19.3",
    "vite": "^7.2.4",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-static-copy": "^3.1.4",
    "wait-on": "^9.0.3"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b"
}
